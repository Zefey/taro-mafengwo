<block wx:if="{{$taroCompReady}}">
    <view class="navbarWrap" style="{{anonymousState__temp}}">
        <block wx:if="{{showLeft}}">
            <view class="iconfont iconleft navLeft" style="{{anonymousState__temp2}}" bindtap="clickHandler"></view>
        </block>
        <view class="title" style="{{anonymousState__temp3}}">{{title}}</view>
        <block wx:if="{{showRelease}}">
            <view class="navRight">
                <view class="releaseBtn" style="{{anonymousState__temp4}}" bindtap="funPrivatefzzzz">
                    <view class="iconfont iconcloud releaseIcon" style="{{anonymousState__temp5}}"></view><text class="releaseText" style="{{anonymousState__temp6}}">发布</text>
                </view>
            </view>
        </block>
    </view>
</block>